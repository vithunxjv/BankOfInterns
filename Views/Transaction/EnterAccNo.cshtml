@model BOI.Models.EnterAccNo

@{ ViewBag.Title = "EnterAccNo"; }

@Styles.Render("~/Content/csuid")


@using (Html.BeginForm("EnterAccNo", "Transaction", FormMethod.Post))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h3>Enter your Account Number</h3>
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            <div class="col-md-10">
                @Html.EditorFor(model => model.AccNo, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.AccNo, "", new { @class = "text-danger" })
            </div>
        </div>


        <div class="form-group-2">
            <label class="error">@Html.DisplayFor(model => model.AccNoErrorMsg)</label>
        </div>

        <div class="form-group-1">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Submit" class="btn btn-default" />
            </div>
        </div>


    </div>}

